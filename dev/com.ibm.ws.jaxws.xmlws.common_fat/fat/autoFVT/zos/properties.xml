<!-- properties based off ${FVT.base} -->           
<property name="cmvc.fvt.base"    value="autoFVT"/>
<property name="cmvc.fvt.dir"     value="${cmvc.fvt.base}"/>
<property name="FVT.base.dir"     value="${FVT.base}/${cmvc.fvt.dir}"/>

<property name="ez.prompt.home"   value="${ez.user.uid}:${ez.user.home}&gt;"/>
<property name="ez.prompt.fvt"    value="${ez.user.uid}:${ez.user.home}/${cmvc.fvt.dir}&gt;"/>
<property name="ez.prompt.login"  value="${ez.user.uid}:"/>

<property name="debug" value="on"/>                                        

<!-- The ending signal from the .sh files which
     usually have long command lines. At the writing moment
     the EzWAS can not take a command line which is over about 40 characters
-->
<property name="endsignal" value="TEL:1-512-838-1879-512-838-1879"/>                                        

<taskdef name="autotelnet" 
         classname="AutoTelnetTask">
  <classpath path="${FVT.zos.dir}/classes" />
</taskdef>

<taskdef name="staf" 
         classname="com.ibm.staf.ant.taskdef.STAF">
  <classpath path="${FVT.zos.dir}/classes"/>
</taskdef>


<condition property="IF.preluninstall">
   <istrue value="${do.preluninstall}"/>
</condition>


<condition property="IF.pluninstall">
   <istrue value="${do.luninstall}"/>
</condition>


<condition property="IF.linstall">
   <istrue value="${do.linstall}"/>
</condition>

<condition property="IF.enable.zos.LTPA">
   <istrue value="${enable.zos.LTPA}"/>
</condition>

