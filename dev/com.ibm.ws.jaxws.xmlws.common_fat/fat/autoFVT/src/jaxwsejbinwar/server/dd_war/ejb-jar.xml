<?xml version="1.0" encoding="UTF-8"?>
<!-- ejb.jar.xml packaged in the web-inf lib dir --> 
<!-- optional, but we'll use it to see if we can affect ejb's in wars  and also to set some service refs -->

<ejb-jar id="ejb-jar_ID" metadata-complete="false" version="3.1"
    xmlns="http://java.sun.com/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee/ejb-jar_3_1.xsd">
    <enterprise-beans>
        
        <session id="ResxxSession1">
            <ejb-name>EjbInWarLibHello</ejb-name>
            <session-type>Stateless</session-type>
          
              <!-- test some service refs.
                 Schema is http://java.sun.com/xml/ns/javaee/javaee_web_services_client_1_3.xsd 
               -->
            <service-ref>
                <service-ref-name>service/metainf_ejbInWarClassesRef</service-ref-name>
                <service-interface>jaxwsejbinwar.generatedclient.EjbInWarClassesHelloService</service-interface>                
            </service-ref>            
            
        </session>       
        
         <session id="ResxxSession21">
            <ejb-name>EjbInWarClassesHello</ejb-name>
            <session-type>Stateless</session-type>
            <ejb-class>jaxwsejbinwar.server.EjbInWarClassesHello</ejb-class>
        </session>
        
    </enterprise-beans>
</ejb-jar>