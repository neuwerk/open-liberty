<webservices
    xmlns="http://java.sun.com/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
             http://java.sun.com/xml/ns/javaee/javaee_web_services_1_2.xsd"
    version="1.2"
>

    <webservice-description>
        <webservice-description-name>WarEcho</webservice-description-name>
        <!-- Default wsdl-file: WEB-INF/wsdl/WarEchoService.wsdl -->
        <wsdl-file>WEB-INF/wsdl/WarEchoService1.wsdl</wsdl-file>
        <port-component>
            <!-- In the WSDL: <portType name="WarEchoOverride"> -->
            <!-- In WarEcho.java: @WebService( name="WarEcho" ) -->
            <port-component-name>WarEcho</port-component-name>
            <enable-mtom>false</enable-mtom>
            <service-impl-bean>
                <servlet-link>jaxwsejb30.ejbapps.serverdepdesc.server.echowar.WarEcho</servlet-link>
            </service-impl-bean>
        </port-component>
    </webservice-description>
    
</webservices>
