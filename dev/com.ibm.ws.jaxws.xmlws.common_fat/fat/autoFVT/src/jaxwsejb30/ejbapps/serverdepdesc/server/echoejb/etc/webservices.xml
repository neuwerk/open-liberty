<webservices
    xmlns="http://java.sun.com/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
             http://java.sun.com/xml/ns/javaee/javaee_web_services_1_2.xsd"
    version="1.2"
>

    <webservice-description>
        <webservice-description-name>EjbEcho</webservice-description-name>
        <!-- Default wsdl-file: META-INF/wsdl/EjbEchoService.wsdl -->
        <wsdl-file>META-INF/wsdl/EjbEchoService1.wsdl</wsdl-file>
        <port-component>
            <!-- In the WSDL: <portType name="EjbEchoOverride"> -->
            <!-- In EjbEcho.java: @WebService( name="EjbEcho" ) -->
            <port-component-name>EjbEchoOverride</port-component-name>
            <!--
            <wsdl-port>EjbEchoPort</wsdl-port>
            <wsdl-service>EjbEchoService</wsdl-service>
            -->
            <enable-mtom>false</enable-mtom>
            <service-impl-bean>
                <ejb-link>EjbEcho</ejb-link>
            </service-impl-bean>
        </port-component>
    </webservice-description>
    
</webservices>