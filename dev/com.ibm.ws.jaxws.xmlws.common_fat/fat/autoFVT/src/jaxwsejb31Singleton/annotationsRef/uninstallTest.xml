<?xml version="1.0"?>

<!DOCTYPE project [
        <!ENTITY instance SYSTEM "../../../instance.xml">
        <!ENTITY properties SYSTEM "../../xmls/properties.xml">
        <!ENTITY targets SYSTEM "../../xmls/targets.xml">
        <!ENTITY taskdefs SYSTEM "../../xmls/taskdefs.xml">
        <!ENTITY taskdefs_post_compile SYSTEM "../../xmls/taskdefs_post_compile.xml">
        <!ENTITY path_refs SYSTEM "../../xmls/path_refs.xml">
]>
<!--
     1.2, 12/3/09
    
    Change History:
    Date       UserId              Feature/Defect          Description
    ****************************************************************************
    11/18/09   varadan             F743-17947-01           New file for Singleton and WebServiceRef
    12/03/09   jtnguyen            F743-17947-01           New file for Singleton and WebServiceRef plus Startup
-->

<project default="uninstall" basedir=".">
   
  <!-- These are XML entity refs.  Basically these statements cause the
       files declared in the DOCTYPE at the top of the file to be imported
       into this build file.
  -->   
  &instance;
  &properties;
  &path_refs;
  &taskdefs;
  &taskdefs_post_compile;
  &targets;

  <!-- The uninstall target will uninstall the EAR or WAR file from
       WebSphere.
  -->

    <target name="uninstall" depends="setenv">
        <UninstallApp appName="jaxwsejb31Singleton-anno-Ejb1"
                      fvtRootLocation="${FVT.base.dir}"
                      uninstallMode="${uninstall.mode}"
                      localInstall="${conn.type}">
        </UninstallApp>
        
        <UninstallApp appName="jaxwsejb31Singleton-anno-Ejb1Startup"
                      fvtRootLocation="${FVT.base.dir}"
                      uninstallMode="${uninstall.mode}"
                      localInstall="${conn.type}">
        </UninstallApp>
        
        <UninstallApp appName="jaxwsejb31Singleton-anno-Ejb2Add"
                      fvtRootLocation="${FVT.base.dir}"
                      uninstallMode="${uninstall.mode}"
                      localInstall="${conn.type}">
        </UninstallApp>
        
    </target>
</project>

