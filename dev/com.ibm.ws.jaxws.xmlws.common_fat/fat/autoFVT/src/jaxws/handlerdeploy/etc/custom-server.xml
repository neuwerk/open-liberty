<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<bindings 
  xmlns:xsd="http://www.w3.org/2001/XMLSchema"
  xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/"
  xmlns:jaxb="http://java.sun.com/xml/ns/jaxb"
  wsdlLocation="EchoMessage.wsdl"
  xmlns="http://java.sun.com/xml/ns/jaxws">

  <bindings node="wsdl:definitions" xmlns:jws="http://java.sun.com/xml/ns/javaee">
    <jws:handler-chains>
      <jws:handler-chain>
      <jws:service-name-pattern xmlns:ns1="http://handlerdeploy.jaxws">ns1:*SimpleService</jws:service-name-pattern>
        <jws:handler>
          <jws:handler-class>jaxws.handlerdeploy.wsfvt.server.ServerLogicalHandler</jws:handler-class>
        </jws:handler>
        <jws:handler>
          <jws:handler-class>jaxws.handlerdeploy.wsfvt.server.ServerSOAPHandler</jws:handler-class>
        </jws:handler>
      </jws:handler-chain>
    </jws:handler-chains>
  </bindings>

</bindings>
