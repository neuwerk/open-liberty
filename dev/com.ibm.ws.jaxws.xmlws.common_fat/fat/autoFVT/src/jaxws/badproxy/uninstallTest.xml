<project default="uninstall" basedir=".">
   
	<property name="componentName" value="jaxws/badproxy"/>
	<property name="test.name" value="jaxws.badproxy" />
	
    <import file="..\jaxws-commons.xml" />

	<property name="test.base.dir" value="${FVT.base.dir}/src/${componentName}" />

  <!-- The uninstall target will uninstall the EAR or WAR file from
       WebSphere.
  -->
  <target name="uninstall" depends="setenv">
   <echo message="Uninstalling ${test.name}App.ear ..." />
   
    <UninstallApp appName="${test.name}.logger"
                  fvtRootLocation="${FVT.base.dir}"
                  uninstallMode="${uninstall.mode}"
                  localInstall="${conn.type}">
    </UninstallApp>
	
    <UninstallApp appName="${test.name}.MismatchedSEI"
                  fvtRootLocation="${FVT.base.dir}"
                  uninstallMode="${uninstall.mode}"
                  localInstall="${conn.type}">
    </UninstallApp>	
	
    <UninstallApp appName="${test.name}.MismatchedSOAP11"
                  fvtRootLocation="${FVT.base.dir}"
                  uninstallMode="${uninstall.mode}"
                  localInstall="${conn.type}">
    </UninstallApp>		
	
    <UninstallApp appName="${test.name}.MismatchedSOAP12"
                  fvtRootLocation="${FVT.base.dir}"
                  uninstallMode="${uninstall.mode}"
                  localInstall="${conn.type}">
    </UninstallApp>		
	
    <UninstallApp appName="${test.name}.MismatchedWsdl"
                  fvtRootLocation="${FVT.base.dir}"
                  uninstallMode="${uninstall.mode}"
                  localInstall="${conn.type}">
    </UninstallApp>		

    <UninstallApp appName="${test.name}.NoWsdlSOAP11"
                  fvtRootLocation="${FVT.base.dir}"
                  uninstallMode="${uninstall.mode}"
                  localInstall="${conn.type}">
    </UninstallApp>		
	
    <UninstallApp appName="${test.name}.NoWsdlSOAP12"
                  fvtRootLocation="${FVT.base.dir}"
                  uninstallMode="${uninstall.mode}"
                  localInstall="${conn.type}">
    </UninstallApp>	

	<UninstallApp appName="${test.name}.NoWsdlHTTP"
                  fvtRootLocation="${FVT.base.dir}"
                  uninstallMode="${uninstall.mode}"
                  localInstall="${conn.type}">
    </UninstallApp>
  	
    <UninstallApp appName="${test.name}.MTOMSoap12"
                  fvtRootLocation="${FVT.base.dir}"
                  uninstallMode="${uninstall.mode}"
                  localInstall="${conn.type}">
    </UninstallApp>	

	<UninstallApp appName="${test.name}.NoBTSoap12"
                  fvtRootLocation="${FVT.base.dir}"
                  uninstallMode="${uninstall.mode}"
                  localInstall="${conn.type}">
    </UninstallApp>
  	
	<UninstallApp appName="${test.name}.AsyncMethods"
                  fvtRootLocation="${FVT.base.dir}"
                  uninstallMode="${uninstall.mode}"
                  localInstall="${conn.type}">
    </UninstallApp>	  
	
  </target>
  
</project>
