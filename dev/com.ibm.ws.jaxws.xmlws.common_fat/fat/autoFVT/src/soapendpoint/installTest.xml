<project default="install" basedir=".">


	<!-- These are XML entity refs.  Basically these statements cause the
       files declared in the DOCTYPE at the top of the file to be imported
       into this build file.
  -->   
  &instance;
  &properties;
  &path_refs;
  &taskdefs;
  &taskdefs_post_compile;
  &targets;

  <!-- The install target will install the EAR or WAR file into WebSphere.
  -->
	<target name="install" depends="setenv">

		<InstallApp earName="SoapEndpoint.ear" appName="SoapEndpoint" localInstall="${conn.type}" useDefaultBindings="true" installRootLocation="${FVT.build.installableApps.dir}" installMode="${install.mode}" fvtRootLocation="${FVT.base.dir}" />

		<InstallApp earName="SoapEndpointInstallError.ear" appName="SoapEndpointInstallError" localInstall="${conn.type}" useDefaultBindings="true" installRootLocation="${FVT.build.installableApps.dir}" installMode="${install.mode}" fvtRootLocation="${FVT.base.dir}" />

	</target>

</project>

