<!ENTITY properties SYSTEM "../xmls/properties.xml">
<!-- file:36 -->
<!ENTITY targets SYSTEM "../xmls/targets.xml">
<!-- file:33 -->
<!ENTITY taskdefs SYSTEM "../xmls/taskdefs.xml">
