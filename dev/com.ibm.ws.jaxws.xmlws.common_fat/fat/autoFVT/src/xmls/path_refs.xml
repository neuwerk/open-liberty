</path>

<path id="2.2api.classpath">  
  <fileset dir="${FVT.base.dir}/common/jars/endorsed">
    <include name="**/*.jar"/>    
  </fileset> 
</path>
<property name="ref.to.22api" refid="2.2api.classpath"/>
<!-- This is a path element that contains paths and jars that are
     are needed by most of the other classpath path elements.

     Be careful when adding additional elements to this classpath
     because it is used by multiple path elements.  In particular,
     the thin client may need to run without a particular jar.
     If that is the case, just add the jar to the thinclientclasspath.
-->
<path id="commonclasspath">
  <pathelement location="${FVT.build.classes.dir}"/>
  <!-- The xml.jar is needed only for iSeries.  It will not
       be found on any other platform.  On other platforms, the JDK
       automatically picks up this jar.
  -->
  <pathelement location="${java.home}/lib/xml.jar"/> <!-- Change to ibm jdk reference, note that oracle jdk does not have this jar -->  
  <path refid="fattest.simplicity.first.classpath"/>
  <pathelement location="${FVT.base.dir}/common/jars/provider.api.jar"/>
  <pathelement location="${FVT.base.dir}/common/jars/provider.wsadmin.jar"/>
  <pathelement location="${FVT.base.dir}/common/jars/provider.jmx.jar"/>
  <pathelement location="${FVT.base.dir}/common/jars/provider.rxa.jar"/>
  <pathelement location="${FVT.base.dir}/common/jars/jiiws.jar"/>
  <pathelement location="${FVT.base.dir}/common/jars/jlanclient.jar"/>
  <pathelement location="${FVT.base.dir}/common/jars/remoteaccess.jar"/>
  <pathelement location="${FVT.base.dir}/common/jars/ssh.jar"/>
  <pathelement location="${FVT.base.dir}/common/jars/JSTAF.jar"/>
  <!-- The following jars are used for the HttpUnit test tool.
  -->
  <pathelement location="${FVT.base.dir}/common/jars/HTTPClient.jar"/>
  <pathelement location="${FVT.base.dir}/common/jars/${ibm.httpunit.jar.name}"/>
  <pathelement location="${FVT.base.dir}/common/jars/nekohtml.jar"/>
  <pathelement location="${JUNIT.base.dir}/junit.jar" />
  <pathelement location="${FVT.base.dir}/common/jars/xmlunit-1.1.jar"/>
  <fileset dir="${FVT.base.dir}/common/jars/endorsed">
    <include name="**/*.jar"/>    
  </fileset>
</path>


<!-- The thin client classpath is used for testing with the
     Web Services Feature Pack thin client com.ibm.jaxws.thinclient_8.5.0.jar
     file.  There is another path id named jaxrpctcclasspath for testing
     with the Maelstrom based JAX-RPC thin client.

     Be careful when adding jars to this classpath.
-->
<!-- Lots of jaxws sub-buckets rely on this classpath -->
<path id="thinclientclasspath">
  <pathelement location="${FVT.base.dir}/common/jars/twas/com.ibm.jaxws.saml.thinclient_1.0.0.jar"/>
  <pathelement location="${FVT.base.dir}/common/jars/twas/com.ibm.jaxws.thinclient_8.5.0.jar"/>
  <pathelement location="${FVT.base.dir}/common/jars/twas/com.ibm.ws.admin.client_8.5.0.jar"/>
  <path refid="commonclasspath"/>
</path>

