<?xml version="1.0"?> 

<project default="uninstall" basedir=".">
    <import file="../../xmls/common_imports.xml"/>
    
  <!-- The uninstall target will uninstall the EAR or WAR file from
       WebSphere.
  -->
     <target name="singleuninstall" depends="setenv">   
	    <antcall target="subuninstall">
		     <param name="appname" value="jaxws22addr_annoOffDDOn.ear"/>
		 </antcall>    	    
   </target>
  <target name="uninstall" depends="setenv">
	    <antcall target="subuninstall">
		     <param name="appname" value="jaxws22addr_annoOffDDOn.ear"/>
		 </antcall>  
	    <antcall target="subuninstall">
		     <param name="appname" value="jaxws22addr_annoOnDDOff.ear"/>
		 </antcall>   
	    <antcall target="subuninstall">
		     <param name="appname" value="jaxws22addr_noAnnoDDOn.ear"/>
		 </antcall>   		 
	    <antcall target="subuninstall">
		     <param name="appname" value="jaxws22addr_annoOnNoDD.ear"/>
		 </antcall>   	
	   <antcall target="subuninstall">
		     <param name="appname" value="jaxws22addr_annoOnImplicitDDOff.ear"/>
		 </antcall> 		 	 		  
  </target>
	     

  <target name="subuninstall" >
	   <echo message="Uninstalling ${appname}..." />
	    <UninstallApp appName="${appname}"
	                  fvtRootLocation="${FVT.base.dir}"
	                  uninstallMode="${uninstall.mode}"
	                  localInstall="${conn.type}">
	    </UninstallApp>  	
  </target>
  
</project>
