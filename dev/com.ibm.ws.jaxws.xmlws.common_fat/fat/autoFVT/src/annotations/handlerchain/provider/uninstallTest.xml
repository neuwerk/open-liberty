<?xml version="1.0"?>
<!DOCTYPE project [
    <!ENTITY instance SYSTEM "../../../../instance.xml"><!-- file:30 -->
<!ENTITY properties SYSTEM "../../../xmls/properties.xml"><!-- file:28 -->
<!ENTITY targets SYSTEM "../../../xmls/targets.xml"><!-- file:25 -->
<!ENTITY taskdefs SYSTEM "../../../xmls/taskdefs.xml"><!-- file:26 -->
<!ENTITY taskdefs_post_compile SYSTEM "../../../xmls/taskdefs_post_compile.xml"><!-- file:39 -->
<!ENTITY path_refs SYSTEM "../../../xmls/path_refs.xml"><!-- file:27 -->
]>

<!--
   1.2, 8/3/07

   Description: This is the uninstall file for the handlerchain/checkdefaults test.

   Change History:
        Date        Author       Feature/Defect          Description
        08/17/2006  euzunca      LIDB3296.31.01          new File
-->

<project default="uninstall" basedir=".">

  &instance;
  &properties;
  &path_refs;
  &taskdefs;
  &taskdefs_post_compile;
  &targets;


  <!-- The uninstall target will uninstall the EAR or WAR file from
       WebSphere.
  -->
  <target name="uninstall" depends="setenv">
      <antcall target="subuninstall" >
           <param name="appname" value="HandlerChainProvider1" />
      </antcall>
  </target>

  <target name="subuninstall" >
    <echo message="Uninstalling ${appname}.ear " />
    <UninstallApp appName="${appname}"
                  fvtRootLocation="${FVT.base.dir}"
                  uninstallMode="${uninstall.mode}"
                  localInstall="${conn.type}">
    </UninstallApp>
  </target>
</project>
