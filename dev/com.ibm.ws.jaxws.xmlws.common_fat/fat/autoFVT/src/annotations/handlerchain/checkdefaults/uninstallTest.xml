<?xml version="1.0"?>
<!DOCTYPE project [
    <!ENTITY instance SYSTEM "../../../../instance.xml"><!-- file:30 -->
        <!ENTITY properties SYSTEM "../../../xmls/properties.xml"><!-- file:36 -->
        <!ENTITY targets SYSTEM "../../../xmls/targets.xml"><!-- file:33 -->
        <!ENTITY taskdefs SYSTEM "../../../xmls/taskdefs.xml"><!-- file:34 -->
        <!ENTITY taskdefs_post_compile SYSTEM "../../../xmls/taskdefs_post_compile.xml"><!-- file:47 -->
        <!ENTITY path_refs SYSTEM "../../../xmls/path_refs.xml"><!-- file:35 -->
]>

<!--
   1.4, 8/3/07

   Description: This is the uninstall file for the handlerchain/checkdefaults test.

   Change History:
        Date        Author       Feature/Defect          Description
        07/20/2006  euzunca      LIDB3296.31.01          new File
-->

<project default="uninstall" basedir=".">

  <!-- These are XML entity refs.  Basically these statements cause the
       files declared in the DOCTYPE at the top of the file to be imported
       into this build file.
  -->
  &instance;
  &properties;
  &path_refs;
  &taskdefs;
  &taskdefs_post_compile;
  &targets;

  <target name="xuninstall" >
     <echo message="disabled until descoped function is present in product" />
  </target>

  <!-- The uninstall target will uninstall the EAR or WAR file from
       WebSphere.
  -->
  <target name="uninstall" depends="setenv">
      <antcall target="subuninstall" >
           <param name="appname" value="HandlerChainCheckDefaults1" />
      </antcall>
      <antcall target="subuninstall" >
           <param name="appname" value="HandlerChainCheckDefaults2" />
      </antcall>
      <antcall target="subuninstall" >
           <param name="appname" value="HandlerChainCheckDefaults3" />
      </antcall>
      <antcall target="subuninstall" >
           <param name="appname" value="HandlerChainCheckDefaults4" />
      </antcall>
      <antcall target="subuninstall" >
           <param name="appname" value="HandlerChainCheckDefaults5" />
      </antcall>
      <antcall target="subuninstall" >
           <param name="appname" value="HandlerChainCheckDefaults6" />
      </antcall>
  </target>

  <target name="subuninstall" >
    <echo message="Uninstalling ${appname}.ear " />
    <UninstallApp appName="${appname}"
                  fvtRootLocation="${FVT.base.dir}"
                  uninstallMode="${uninstall.mode}"
                  localInstall="${conn.type}">
    </UninstallApp>
  </target>

</project>
