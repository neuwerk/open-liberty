   </servlet>
   <servlet-mapping>
         <servlet-name>annotations.webservice_g2.inheritance.server.BottomImplWithSei</servlet-name>
         <url-pattern>/BottomImplWithSeiService</url-pattern>
   </servlet-mapping>
