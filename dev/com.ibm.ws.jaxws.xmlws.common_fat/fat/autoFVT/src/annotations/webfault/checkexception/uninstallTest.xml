<project default="uninstall" basedir=".">
   
  <!-- These are XML entity refs.  Basically these statements cause the
       files declared in the DOCTYPE at the top of the file to be imported
       into this build file.
  -->   
  &instance;
  &properties;
  &path_refs;
  &taskdefs;
  &taskdefs_post_compile;
  &targets;

  <!-- The uninstall target will uninstall the EAR or WAR file from
       WebSphere.
  -->
  <target name="uninstall" depends="setenv">
    <UninstallApp appName="WebFaultCheckExc"
                  fvtRootLocation="${FVT.base.dir}"
                  uninstallMode="${uninstall.mode}"
                  localInstall="${conn.type}">
    </UninstallApp>
  </target>
  
</project>
