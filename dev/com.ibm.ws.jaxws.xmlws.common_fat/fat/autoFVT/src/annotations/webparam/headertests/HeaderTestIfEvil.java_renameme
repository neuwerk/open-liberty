// this class is used to generate a wsdl to invoke the client
// in a way that will switch body and header parts of messages.
// it is not used by the server.
package annotations.webparam.headertests;
import javax.jws.*;
import javax.jws.soap.*;
@WebService
@SOAPBinding(parameterStyle = SOAPBinding.ParameterStyle.BARE)
public interface HeaderTestIf{

   // input moved to body
   public String inputHeaderOutputBody(   String s );

   // input moved to header
   public String inputBodyOutputBody( @WebParam(header=true) String s   );


   // output moved to header
   @WebResult(header=true)
   public String nextInputBodyOutputBody( String s   );

   // output moved to body
   //@WebResult(header=true)
   public String inputBodyOutputHeader( String s );

  /*
   @WebResult(header=true)
   public String inputHeaderOutputHeader( @WebParam(header=true)  String s);
   */
}
